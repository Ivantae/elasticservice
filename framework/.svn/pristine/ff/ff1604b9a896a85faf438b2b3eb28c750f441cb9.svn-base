<select>
select 
	a.USER_ID as USER_ID
from TB_ELUSER_AUTH A
	INNER JOIN TB_ELUSER B 
		ON a.USER_ID = b.USER_ID
	INNER JOIN TB_ELAUTH C
		ON a.AUTH_ID = c.AUTH_ID
where 
	a.USER_ID = #{USER_ID}
	and b.LOGIN_PWD = #{LOGIN_PWD}
	<dynamic>
		<isEqual prepend="AND" property="ADMIN" compareValue="1">
	a.AUTH_ID = '00'
		</isEqual>
		<isNotEqual prepend="AND" property="ADMIN" compareValue="1">
   			<![CDATA[
	a.AUTH_ID <> '00'
 			]]>
		</isNotEqual>
	</dynamic>
</select>